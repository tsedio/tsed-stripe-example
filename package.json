    "@types/cors": "^2.8.13",
    "@types/dotenv-flow": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "tsed": {
    "convention": "conv_default",
